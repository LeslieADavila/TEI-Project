         <listPerson>
            <person xml:id="JuanCGuerra">
               <persName xml:id="JuanCGuerra-name1">Juan Chapa Guerra</persName>
               <death>
                  <date when="1850-01">Jan. 1850</date>
                  <location>
                     <measure>Near</measure>
                     <placeName ref="Bville">Brownsville</placeName>
                  </location>.
               </death>
               <listEvent>
                  <event>
                     <desc>
                        <persName ref="#JuanCGuerra">Juan Chapa Guerra</persName> was lynched on <date when="1850-01">Jan. 1850</date> near <location><placeName ref="Bville">Brownsville</placeName></location>.
                        He was charged with theft. He was whipped and then killed by a small group of men under mistaken 
                        impression that victim was Juan Chapa Garcia.
                     </desc>
                  </event>
               </listEvent>
               <bibl xml:id="biblynched-1">
                  <title>Report of the Committee of Investigation of the Mexican Government</title>
               </bibl>
            </person>
         </listPerson>
